package enums;

public enum Message {
	SYMMETRIC_KEY_GENERATED("대칭키를 생성했습니다."),
	ID_NOT_EXISTS("병원의 아이디가 존재하지 않습니다."),
	DE_CREATED("전자봉투를 생성했습니다."),
	TRANSMISSION_CHOICE("전자봉투를 연구소로 보내시겠습니까? y/n (나가기 q) : "),
	YES("y"),
	NO("n"),
	QUIT("q"),
	TRANSMISSION_CANCELED("파일 전송을 취소했습니다. 파일은 삭제되지 않습니다."),
	WRONG_INPUT("입력이 잘못되었습니다."),
	SIGN_RESULT("\n서명 검증 결과 : "),
	MEDICAL_RECORD("<의료 기록>"),
	PROGRAM_TERMINATE("프로그램이 종료됩니다.");
	
	private final String message;

	Message(String message) {
        this.message = message;
    }

    public String getMessage() {
        return message;
    }
}
