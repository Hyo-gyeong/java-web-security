package enums;

public enum FilePath {
	KEY_MANAGER("./src/key_manager/"),
	PRIVATE_KEY(KEY_MANAGER+"privateKeys/private_"),
	PUBLIC_KEY(KEY_MANAGER+"publicKeys/public_"),
	HOSPITAL_ENCRYPTED_SECRET_KEY("./src/hospital/encrypted_secret_key.txt"),
	HOSPITAL_SECRET_HASHED_MEDICAL_RECORD("./src/hospital/secret_hashed_medical_record.txt"),
	;
	
	private final String path;

	FilePath(String path) {
        this.path = path;
    }

    public String getPath() {
        return path;
    }
}
