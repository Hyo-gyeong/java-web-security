package enums;

public enum Message {
	INIT_OBJECT_AND_KEY("객체 및 키를 초기화합니다."),
	MEDICAL_OBJECT_CREATED("의사 정보, 환자 정보, 의료 정보를 생성했습니다."),
	RESEARCH_OBJECT_CREATED("연구소 정보를 생성했습니다."),
	OBJECT_CREATED_SUCCESS("------- 모든 객체 생성이 성공적으로 완료되었습니다. -------"),
	KEY_CREATED_SUCCESS("------- 모든 키 생성이 성공적으로 완료되었습니다. -------"),
	HOSPITAL_GREETING("안녕하세요. A병원 서버입니다.\n의료 기록을 암호화 하려면 비밀번호를 입력하세요: "),
	SYMMETRIC_KEY_GENERATED("대칭키를 생성했습니다."),
	HOPITAL_ID_NOT_EXISTS("병원의 아이디가 존재하지 않습니다."),
	RC_ID_NOT_EXISTS("연구소의 아이디가 존재하지 않습니다."),
	DE_CREATED("전자봉투를 생성했습니다."),
	TRANSMISSION_CHOICE("전자봉투를 연구소로 보내시겠습니까? y/n (나가기 q) : "),
	YES("y"),
	NO("n"),
	QUIT("q"),
	TRANSMISSION_CANCELED("파일 전송을 취소했습니다. 파일은 삭제되지 않습니다."),
	WRONG_INPUT("입력이 잘못되었습니다."),
	SIGN_RESULT("\n서명 검증 결과 : "),
	MEDICAL_RECORD("<의료 기록>"),
	PROGRAM_TERMINATE("프로그램이 종료됩니다.");
	
	private final String message;

	Message(String message) {
        this.message = message;
    }

    public String getMessage() {
        return message;
    }
}
