package print;

public class print {
	public static String bytesToHex(byte[] bytes) { //test
		StringBuilder result = new StringBuilder();
		int i = 0;
		for (byte b : bytes) {
			if (i != 0 && i % 20 == 0) {
				result.append("\n");
			}
			result.append(String.format("%02X ", b));
			i++;
		}
		return result.toString();
	}
}
