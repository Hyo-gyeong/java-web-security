// 컴퓨터학과, 20190975, 신효경
package enums;

public enum Algorithm {
    // 서명 알고리즘
    SIGN_ALGORITHM("SHA1withRSA"),
    
    // 해시 알고리즘
    MD_ALGORITHM("SHA-1"),
    
    // 대칭 키 암호화 알고리즘
    CIPHER_ALGORITHM("AES"),
    
    // 대칭 키 알고리즘과 길이
    SK_ALGORITHM("AES"),
    SK_LENGTH("128");
    
    private String algorithm;
    
    // 생성자
    Algorithm(String algorithm){
        this.algorithm = algorithm;
    }
    
    // 알고리즘 반환 메서드
    public String getAlgorithm() {
        return algorithm;
    }
}

