package crypto_manager;

import java.io.IOException;
import java.security.MessageDigest;
import java.security.NoSuchAlgorithmException;

import enums.Algorithm;

public class Hash {
	public static byte[] SHA1HashMedicalRecord(String mr) throws NoSuchAlgorithmException, IOException {
		byte[] digest = null;
		
		MessageDigest md = MessageDigest.getInstance(Algorithm.AES.getAlgorithm());
		md.update(mr.getBytes());
		digest = md.digest();
		return digest;
	}
}
